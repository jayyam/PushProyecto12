CREATE TABLE 'products' (
  'id' int(11) NOT NULL PRIMARY KEY AUTO_INCREMENT,
  'type' char(1) COLLATE latin1_spanish_ci NOT NULL,
  'name' varchar(200) COLLATE latin1_spanish_ci NOT NULL,
  'description' text COLLATE latin1_spanish_ci NOT NULL,
  'price' decimal(10,2) NOT NULL,
  'discount' decimal(10,2) NOT NULL,
  'send' decimal(10,2) NOT NULL,
  'image' varchar(100) COLLATE latin1_spanish_ci NOT NULL,
  'published' date NOT NULL,
  'relation1' int(11) NOT NULL,
  'relation2' int(11) NOT NULL,
  'relation3' int(11) NOT NULL,
  'mostSold' char(1) COLLATE latin1_spanish_ci NOT NULL,
  'new' char(1) COLLATE latin1_spanish_ci NOT NULL,
  'status' tinyint(4) NOT NULL,
  'deleted' tinyint(4) NOT NULL,
  'create_at' datetime NOT NULL,
  'updated_at' datetime,
  'deleted_at' datetime,
  'author' varchar(200) COLLATE latin1_spanish_ci NOT NULL,
  'publisher' varchar(200) COLLATE latin1_spanish_ci NOT NULL,
  'pages' int(11) NOT NULL,
  'people' text COLLATE latin1_spanish_ci NOT NULL,
  'objetives' text COLLATE latin1_spanish_ci NOT NULL,
  'necesites' text COLLATE latin1_spanish_ci NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;
